{
    "name": "agiuscloud/do-oauth-creator",
    "description": "A php wrapper for creating DigitalOcean client account keys for use with an application.",
    "license": "MIT",
    "keywords": ["DigitalOcean", "Oauth", "PHP", "Laravel"],
    "authors": [
        {
            "name": "Tyler Youschak",
            "email": "tyleryouschak@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "suggest": {
        "illuminate/support": "Enables Laravel support (~5.0)."
    },
    "autoload": {
        "psr-4": {
            "WebStax\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebStax\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
